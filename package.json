{
  "name": "webpack-antd-build",
  "version": "1.0.0",
  "private": true,
  "description": "A webpack antd build tools.",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "dev": "node webpack/bin/webpack.config.dev.js",
    "prod": "node webpack/bin/webpack.config.prod.js",
    "watch": "nodemon webpack/bin/webpack.config.server.js",
    "report": "node webpack/bin/webpack.config.prod.js --report",
    "format": "prettier --write .babelrc.js .postcssrc.js webpack Assets"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE >= 0"
  ],
  "nodemonConfig": {
    "quiet": true,
    "delay": "100ms",
    "watch": [
      ".babelrc.js",
      "webpack/**/*",
      ".postcssrc.js",
      "wwwroot/favicon.ico",
      "Assets/css/theme.less"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/plots": "^1.0.9",
    "antd": "^4.20.0",
    "classnames": "^2.3.1",
    "core-js": "^3.22.2",
    "dayjs": "^1.11.1",
    "memoize-one": "^6.0.0",
    "rc-virtual-list": "^3.4.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-nest-router": "^0.1.1",
    "throttle-debounce": "^4.0.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@svgr/webpack": "^6.2.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2",
    "@types/throttle-debounce": "^4.0.0",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "3.4.1",
    "css-modules-types-loader": "^0.1.1",
    "find-free-ports": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-webpack-dev-service": "^0.2.5",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "memfs": "^3.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}