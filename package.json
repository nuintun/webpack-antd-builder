{
  "name": "webpack-antd-build",
  "version": "1.0.0",
  "description": "A webpack antd build tools.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "format": "prettier --write .",
    "dev": "node webpack/bin/webpack.config.dev.js",
    "prod": "node webpack/bin/webpack.config.prod.js",
    "watch": "nodemon webpack/bin/webpack.config.server.js",
    "report": "node webpack/bin/webpack.config.prod.js --report"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "nodemonConfig": {
    "quiet": true,
    "delay": "128ms",
    "watch": [
      ".swcrc.js",
      "webpack/**/*",
      ".postcssrc.js",
      ".browserslistrc",
      "wwwroot/favicon.ico"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.0.0",
    "@ant-design/icons": "^4.8.0",
    "@ant-design/plots": "^1.2.2",
    "antd": "^5.0.1",
    "classnames": "^2.3.2",
    "core-js": "^3.26.1",
    "dayjs": "^1.11.6",
    "memoize-one": "^6.0.0",
    "rc-virtual-list": "^3.4.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-nest-router": "^0.18.1",
    "throttle-debounce": "^5.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@swc/core": "^1.3.19",
    "@swc/helpers": "^0.4.14",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/throttle-debounce": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "4.2.2",
    "css-modules-types-loader": "^0.3.8",
    "find-free-ports": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-webpack-dev-service": "^0.8.0",
    "memfs": "^3.4.12",
    "mini-css-extract-plugin": "^2.7.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
