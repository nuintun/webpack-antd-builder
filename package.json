{
  "name": "webpack-antd-build",
  "version": "1.0.0",
  "description": "A webpack antd build tools.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "format": "prettier --write .",
    "dev": "node webpack/bin/webpack.config.dev.js",
    "prod": "node webpack/bin/webpack.config.prod.js",
    "watch": "nodemon webpack/bin/webpack.config.server.js",
    "report": "node webpack/bin/webpack.config.prod.js --report"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE >= 0"
  ],
  "nodemonConfig": {
    "quiet": true,
    "delay": "128ms",
    "watch": [
      "webpack/**/*",
      ".babelrc.cjs",
      ".postcssrc.cjs",
      "wwwroot/favicon.ico",
      "Assets/css/theme.less"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/plots": "^1.2.2",
    "antd": "^4.24.2",
    "classnames": "^2.3.2",
    "core-js": "^3.26.1",
    "dayjs": "^1.11.6",
    "memoize-one": "^6.0.0",
    "rc-virtual-list": "^3.4.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-nest-router": "^0.18.0",
    "throttle-debounce": "^5.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/throttle-debounce": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "babel-plugin-import": "^1.13.5",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "4.2.2",
    "css-modules-types-loader": "^0.3.8",
    "find-free-ports": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "koa-webpack-dev-service": "^0.8.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "memfs": "^3.4.11",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
