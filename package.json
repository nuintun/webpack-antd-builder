{
  "name": "webpack-antd-build",
  "version": "1.0.0",
  "private": true,
  "description": "A webpack antd build tools.",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "format": "prettier --write Assets",
    "dev": "node webpack/bin/webpack.config.dev.js",
    "prod": "node webpack/bin/webpack.config.prod.js",
    "watch": "nodemon webpack/bin/webpack.config.server.js",
    "report": "webpack-bundle-analyzer --port auto node_modules/.cache/webpack-analyzer/report.json wwwroot/public"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "browserslist": [
    "defaults"
  ],
  "nodemonConfig": {
    "quiet": true,
    "delay": "100ms",
    "watch": [
      "webpack/**/*",
      "wwwroot/favicon.ico",
      "Assets/css/theme.less"
    ]
  },
  "resolutions": {
    "react-reconciler": "^0.26.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "antd": "^4.16.8",
    "bizcharts": "^4.1.11",
    "classnames": "^2.3.1",
    "core-js": "^3.15.2",
    "dayjs": "^1.10.6",
    "memoize-one": "^5.2.1",
    "rc-virtual-list": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "throttle-debounce": "^3.0.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/throttle-debounce": "^2.1.0",
    "@types/webpack-env": "^1.16.2",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "3.0.2",
    "find-free-ports": "^2.0.3",
    "html-webpack-plugin": "^5.3.2",
    "koa": "^2.13.1",
    "koa-compress": "^5.0.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.1.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server-middleware": "^0.3.1"
  }
}