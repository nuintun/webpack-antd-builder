{
  "name": "webpack-antd-build",
  "version": "1.0.0",
  "private": true,
  "description": "A webpack antd build tools.",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "format": "prettier --write Assets",
    "dev": "node webpack/bin/webpack.config.dev.js",
    "prod": "node webpack/bin/webpack.config.prod.js",
    "watch": "nodemon webpack/bin/webpack.config.server.js",
    "report": "node webpack/bin/webpack.config.prod.js --report"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "browserslist": [
    "defaults"
  ],
  "nodemonConfig": {
    "quiet": true,
    "delay": "100ms",
    "watch": [
      "webpack/**/*",
      "wwwroot/favicon.ico",
      "Assets/css/theme.less"
    ]
  },
  "resolutions": {
    "@antv/g2": "^4.1.31",
    "@antv/g2plot": "^2.3.37",
    "react-error-boundary": "^3.1.3",
    "react-reconciler": "^0.26.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "bizcharts": "^4.1.14",
    "classnames": "^2.3.1",
    "core-js": "^3.18.3",
    "dayjs": "^1.10.7",
    "memoize-one": "^6.0.0",
    "rc-virtual-list": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "throttle-debounce": "^3.0.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@svgr/webpack": "next",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@types/throttle-debounce": "^2.1.0",
    "@types/webpack-env": "^1.16.3",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "3.1.1",
    "find-free-ports": "^3.0.0",
    "html-webpack-plugin": "^5.4.0",
    "koa": "^2.13.4",
    "koa-compress": "^5.1.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.2",
    "nodemon": "^2.0.14",
    "postcss": "^8.3.10",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server-middleware": "^0.7.1"
  }
}