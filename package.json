{
  "name": "webpack-antd-build",
  "version": "1.0.0",
  "description": "A webpack antd build tools.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/nuintun/webpack-antd-builder",
  "nodemonConfig": {
    "quiet": true,
    "delay": "128ms",
    "watch": [
      ".swcrc.js",
      "tools/**/*",
      ".postcssrc.js",
      "app.config.js",
      ".browserslistrc",
      "wwwroot/favicon.ico"
    ]
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "format": "prettier --write .",
    "prod": "node tools/bin/webpack.config.prod.js",
    "dev": "nodemon tools/bin/webpack.config.dev.js",
    "report": "node tools/bin/webpack.config.prod.js --report"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.7.1",
    "@ant-design/icons": "^5.0.1",
    "@ant-design/plots": "^1.2.5",
    "antd": "^5.3.2",
    "classnames": "^2.3.2",
    "core-js": "^3.29.1",
    "dayjs": "^1.11.7",
    "memoize-one": "^6.0.0",
    "rc-virtual-list": "^3.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.3",
    "react-nest-router": "^0.19.2",
    "throttle-debounce": "^5.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@nuintun/svgo-loader": "^0.1.1",
    "@swc/core": "^1.3.42",
    "@swc/helpers": "^0.4.14",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@types/throttle-debounce": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "bufferutil": "^4.0.7",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "5.0.0",
    "css-modules-types-loader": "^0.3.13",
    "find-free-ports": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "koa": "^2.14.1",
    "koa-compress": "^5.1.0",
    "koa-webpack-dev-service": "^0.9.17",
    "memfs": "^3.4.13",
    "mini-css-extract-plugin": "^2.7.5",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "svgc-loader": "^0.1.5",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.7",
    "typescript": "^5.0.2",
    "utf-8-validate": "^6.0.3",
    "webpack": "^5.76.3",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
